<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SalesManager.CreateNewCompany"
    tools:layout_editor_absoluteY="25dp">


    <include
        android:id="@+id/include5"
        layout="@layout/toolbar" />

    <TextView
        android:id="@+id/title"
        android:layout_width="194dp"
        android:layout_height="28dp"
        android:text="Create New Channel"
        android:textColor="@color/quantum_black_100"
        android:textSize="21sp"
        tools:layout_editor_absoluteX="126dp"
        tools:layout_editor_absoluteY="56dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Company Name:"
        tools:layout_editor_absoluteX="39dp"
        tools:layout_editor_absoluteY="208dp" />

    <EditText
        android:id="@+id/companyName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="eg: Telekom Sdn Bhd"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="182dp"
        tools:layout_editor_absoluteY="196dp" />

    <EditText
        android:id="@+id/passcode"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPassword"
        tools:layout_editor_absoluteX="182dp"
        tools:layout_editor_absoluteY="349dp" />

    <EditText
        android:id="@+id/passcode_confirm"
        android:layout_width="wrap_content"
        android:layout_height="44dp"
        android:ems="10"
        android:inputType="textPassword"
        tools:layout_editor_absoluteX="182dp"
        tools:layout_editor_absoluteY="434dp" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Passcode"
        tools:layout_editor_absoluteX="49dp"
        tools:layout_editor_absoluteY="364dp" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="93dp"
        android:layout_height="48dp"
        android:text="Passcode Confirmagtion"
        tools:layout_editor_absoluteX="33dp"
        tools:layout_editor_absoluteY="430dp" />

    <Button
        android:id="@+id/btn_createCompany"
        android:layout_width="124dp"
        android:layout_height="48dp"
        android:text="Submit"
        tools:layout_editor_absoluteX="144dp"
        tools:layout_editor_absoluteY="519dp" />

    <TextView
        android:id="@+id/textViewCompanyId"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Company ID"
        tools:layout_editor_absoluteX="60dp"
        tools:layout_editor_absoluteY="116dp" />

    <EditText
        android:id="@+id/companyId"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="182dp"
        tools:layout_editor_absoluteY="104dp" />

    <TextView
        android:id="@+id/textViewCompanyType"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Company Type"
        tools:layout_editor_absoluteX="60dp"
        tools:layout_editor_absoluteY="288dp" />

    <EditText
        android:id="@+id/companyType"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Name"
        tools:layout_editor_absoluteX="181dp"
        tools:layout_editor_absoluteY="274dp" />

</android.support.constraint.ConstraintLayout>